SHELL = @SHELL@
include ../../Makerules
top_srcdir=@top_srcdir@
srcdir=@srcdir@
@VPATH@

INCLUDES   = -I${top_srcdir}/include/textfilt -I${top_srcdir}/include

LIBS = ../../lib/libtfilter.a 

EXECS = mapnames

default: $(EXECS)

mapnames: mapnames.o
	$(CCC) -o mapnames mapnames.o $(LIBS)
.c.o:
	$(CC) $(CFLAGS) -c $*.c

doc_SOURCES = 
wwwpages:
manpages:

clean:
	/bin/rm -f *.o *~ $(EXECS) ccc
