ALL: lib
SHELL       = @SHELL@

include ../../Makerules
top_srcdir=@top_srcdir@
srcdir=@srcdir@
@VPATH@

INCLUDES = -I${top_srcdir}/include

OBJS = arch.o txt.o daytime.o file.o tr.o getopts.o

LIBS      =  
LDIR      = 
CLEANFILES = 

BASEOBJS = 

clean:
	-/bin/rm -f *.o *~ $(CLEANFILES)
.c.o:
	$(C_COMPILE) -c $<
	$(AR) cr ../../lib/libsowing.a $*.o
lib: $(OBJS)
	$(RANLIB) ../../lib/libsowing.a

doc_SOURCES = 
manpages:

wwwpages:

source_names:
	@echo $(CSOURCES)

