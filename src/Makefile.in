ALL: tohtml
SHELL = @SHELL@
SUBDIRS = sys bfort tohtml doctext textfilt mapnames

clean:
	for dir in $(SUBDIRS) ; do ( cd $$dir ; $(MAKE) clean ) ; done

manpages:
	for dir in $(SUBDIRS) ; do ( cd $$dir ; $(MAKE) manpages ) ; done

wwwpages:
	for dir in $(SUBDIRS) ; do ( cd $$dir ; $(MAKE) wwwpages ) ; done

